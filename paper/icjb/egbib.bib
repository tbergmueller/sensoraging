@INPROCEEDINGS{datingImages,
     author = {Goljan, Miroslav and Fridrich, Jessica},
      title = {Determining Approximate Age of Digital Images Using Sensor Defects},
  booktitle = {SPIE Conference on Media Watermarking, Security, and Forensics},
       year = {2011},
   location = {San Francisco, CA},
   abstract = {New pixel defects occurring in imaging sensors during their lifetime provide valuable forensics information. This study shows how such defects can be used in estimating acquisition time of images originated from digital cameras. While photo response non-uniformity noise has been used for camera identification thanks to its strong stability over time, pixel defects can occur suddenly, for example as a consequence of cosmic radiation. In this work, we model pixel defects as abrupt changes in parameterized model of pixels response. We describe a maximum-likelihood estimator of sensor defects separated into photo-response non-uniformity, dark current, and pixel offset before and after the defect occurs. Then we demonstrate that such estimators can be successfully used to place an image of unknown acquisition time among n other images whose acquisition time is known. Estimation errors depend on n and the frequency of defects occurrences.}
}


